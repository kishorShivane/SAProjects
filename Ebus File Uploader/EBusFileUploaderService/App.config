<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, 	log4net"/>
	</configSections>
	<appSettings>
		<!--PAVAN-->
		<!--mention download path then ftp adress *** DO NOT FORGET TO END WITH SLASH in ftp and local paths (\ or /)-->
		<add key="ebusFtpUploadAddress" value="ftp://154.0.162.166/Pavan/FTP-testing-upload/"/>
		<add key="ebusFtpUploadAddressAlternative" value="ftp://154.0.162.166/Pavan/FTP-testing-upload/BackUp"/>
		<add key="depoLocalFolderPath" value="C:\zz\"/>
		<add key="fileBackUpPath" value="C:\eBusModuleBackup\"/>
		<add key="ebusFtpUserName" value="ebusftp"/>
		<add key="ebusFtpUserPassword" value="e8us@ftp"/>
		<add key="TimeInterval" value="30"/>
		<!--Minutes-->
	</appSettings>
	<!--DONT CHANGE ANYTHING HERE.. JUST MAKE SURE LOG FOLDER EXISTS INSIDE C DRIVE-->
	<log4net>
		<appender name="FileAppender" type="log4net.Appender.RollingFileAppender,log4net">
			<file value="c:\\Log\\ebusLogs.txt" />
			<rollingStyle value="Date" />
			<datePattern value="yyyyMMdd" />
			<appendToFile value="true" />
			<lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %level %logger - %message%newline" />
			</layout>
			<filter type="log4net.Filter.LevelRangeFilter">
				<levelMin value="INFO" />
				<levelMax value="FATAL" />
			</filter>
		</appender>
		<root>
			<level value="DEBUG"/>
			<appender-ref ref="FileAppender"/>
		</root>
	</log4net>
</configuration>